<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.bluemobi.dylan.sqlite">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateOrOpenActivity" />
        <activity android:name=".SqlOperateActivity" />
        <activity android:name=".ApiOperateActivity" />
        <activity android:name=".OptimizeActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".UtilTestActivity" />
    </application>

</manifest>